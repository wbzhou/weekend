<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.littleWeekend.dao.cluster.CityDao">


	<select id="findByName" resultMap="com.littleWeekend.domain.City" parameterType="java.lang.String">
		select * from city
		where city_name = #{cityName}
	</select>

</mapper>
